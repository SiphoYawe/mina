# Reown AppKit Project ID
# Get your project ID at https://dashboard.reown.com
NEXT_PUBLIC_REOWN_PROJECT_ID=your_project_id_here

# Pear Protocol API Client ID
# Whitelisted IDs for hackathon: HLHackathon1 through HLHackathon10
# See: https://docs.pearprotocol.io/api-integration/api-specification/authentication
NEXT_PUBLIC_PEAR_CLIENT_ID=HLHackathon1

# LI.FI API Key (optional, for higher rate limits)
# Get your API key at https://li.fi/
NEXT_PUBLIC_LIFI_API_KEY=

# VAPID Public Key for Web Push Notifications
# Generate keys using: npx web-push generate-vapid-keys
# For demo purposes, a placeholder key is used by default
NEXT_PUBLIC_VAPID_PUBLIC_KEY=
